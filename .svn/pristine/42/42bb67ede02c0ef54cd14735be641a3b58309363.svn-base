package com.caafc.pbocAnalysis.util;

/**
 * 验证返回结果
 * @author zl
 * @version $Revision:$
 */
public class ValidityMessage {

	private boolean result=true;//验证结果  通过true 未通过false
	
	private String errorCode;
	private String errorMessage;//验证未通过原因
	
	
	public ValidityMessage() {
	}

	/**
	 * @param result
	 * @param message
	 */
	public ValidityMessage(boolean result, String errorMessage) {
		this.result = result;
		this.errorMessage = errorMessage;
	}

	/**
	 * @return the result
	 */
	public boolean getResult() {
		return result;
	}

	/**
	 * @param result the result to set
	 */
	public void setResult(boolean result) {
		this.result = result;
	}

	/**
	 * @return the errorMessage
	 */
	public String getErrorMessage() {
		return errorMessage;
	}

	/**
	 * @param errorMessage the errorMessage to set
	 */
	public void setErrorMessage(String errorMessage) {
		this.errorMessage = errorMessage;
	}

	/**
	 * @return the errorCode
	 */
	public String getErrorCode() {
		return errorCode;
	}

	/**
	 * @param errorCode the errorCode to set
	 */
	public void setErrorCode(String errorCode) {
		this.errorCode = errorCode;
	}
	
	
	
	
	
	
	
}
