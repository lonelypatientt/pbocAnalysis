package com.caafc.pbocAnalysis.util;

/**
 * @author xihn 五级分类转换
 */
public class ChangeFiveClass {
	public static String fiveClass(String s) {
		if (s.equals("正常")) {
			return "1";
		} else if (s.equals("关注")) {
			return "2";
		} else if (s.equals("次级")) {
			return "3";
		} else if (s.equals("可疑")) {
			return "4";
		} else if (s.equals("损失")) {
			return "5";
		} else if (s.equals("未知")) {
			return "9";
		}
		return "";
	}
}
