package com.caafc.pbocAnalysis.util;

/**
 * @author xihn 强制执行记录的结案方式转换
 */
public class ChangeEnforceType {
	public static String enforceType(String s) {
		if (s.equals("不予执行")) {
			return "001";
		} else if (s.equals("自动履行")) {
			return "002";
		} else if (s.equals("和解履行完毕")) {
			return "003";
		} else if (s.equals("执行完毕")) {
			return "004";
		} else if (s.equals("终结执行")) {
			return "005";
		} else if (s.equals("提级执行")) {
			return "006";
		} else if (s.equals("指定执行")) {
			return "007";
		} else if (s.equals("其他")) {
			return "255";
		} 
		return "";
	}
}
